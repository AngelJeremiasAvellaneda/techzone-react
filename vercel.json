{
  "version": 2,
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "headers": { "cache-control": "public, max-age=86400" },
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.webmanifest",
      "headers": { "cache-control": "public, max-age=86400" },
      "dest": "/manifest.webmanifest"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
